<div class="find-jobs">
    <div class="container">
        <div class="row">
            <!--侧边导航开始-->
            <div class="nav-menu">
                <div>
                    <div class="menu-title">
                        <img src="../img/chanpin2.png" alt="" class="initial-img" width="18" height="18">
                        <img src="../img/chanpin1.png" alt="" class="hover-img" width="18" height="18">
                        <span>用户体验</span>
                        <img src="../img/zhiweijiantou.png" class="initial-img" width="7" height="13">
                        <img src="../img/zhiweijiantou2.png" class="hover-img" width="7" height="13">
                    </div>
                    <a ng-repeat="x in vm.experienceJobs">{{x.name}}</a>
                    <div class="hover-menu">
                        <ul>
                            <li ng-repeat="x in vm.experienceJobs">
                                <span>{{x.name}}</span>
                                <a ng-repeat="y in x.subCategory">{{y.name}}</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div>
                    <div class="menu-title">
                        <img src="../img/jishu2.png" alt="" class="initial-img" width="18" height="16">
                        <img src="../img/jishu1.png" alt="" class="hover-img" width="18" height="16">
                        <span>研发</span>
                        <img src="../img/zhiweijiantou.png" class="initial-img" width="7" height="13">
                        <img src="../img/zhiweijiantou2.png" class="hover-img" width="7" height="13">
                    </div>
                    <a ng-repeat="x in vm.researchJobs">{{x.name}}</a>
                    <div class="hover-menu">
                        <ul>
                            <li ng-repeat="x in vm.researchJobs">
                                <span>{{x.name}}</span>
                                <a ng-repeat="y in x.subCategory">{{y.name}}</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div>
                    <div class="menu-title">
                        <img src="../img/sheji1.png" alt="" class="initial-img" width="21" height="18">
                        <img src="../img/sheji2.png" alt="" class="hover-img" width="21" height="18">
                        <span>大数据</span>
                        <img src="../img/zhiweijiantou.png" class="initial-img" width="7" height="13">
                        <img src="../img/zhiweijiantou2.png" class="hover-img" width="7" height="13">
                    </div>
                    <a ng-repeat="x in vm.dataJobs">{{x.name}}</a>
                    <div class="hover-menu">
                        <ul>
                            <li ng-repeat="x in vm.dataJobs">
                                <span>{{x.name}}</span>
                                <a ng-repeat="y in x.subCategory">{{y.name}}</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!--侧边导航结束-->
            <!--找职位banner轮播开始-->
            <div class="job-carousel">
                <div uib-carousel active="0" interval="3000">
                    <div uib-slide ng-repeat="slide in vm.slides " index="$index">
                        <img ng-src="{{slide.img}}">
                    </div>
                </div>
                <!--顶部搜索栏开始-->
                <div class="search-box">
                    <form>
                        <div class="input-group">
                            <span class="input-group-addon">公司名称</span>
                            <input class="form-control" placeholder="请输入公司名称">
                            <span class="input-group-addon">
                                <button>搜索</button>
                            </span>
                        </div>
                    </form>
                </div>
                <!--顶部搜索栏结束-->
            </div>
            <!--找职位banner轮播结束-->
        </div>
        <div class="row">
            <div class="col-xs-12 job-title">
                <a ng-class="{selected:!vm.selected}" ng-click="vm.changeSelect(!vm.selected)">推荐职位</a>
                <a ng-class="{selected: vm.selected}" ng-click="vm.changeSelect(vm.selected)">最新职位</a>
                <span class="pull-right">查看更多 ></span>
            </div>
            <div class="col-xs-12 job-content" ng-if="!vm.selected">
                <div class="col-lg-3 col-md-6 padding-0" ng-repeat="x in vm.recommendJob">
                    <div class="job-box">
                        <div class="job-logo">
                            <img ng-src="{{x.logo}}" alt="缩略图">
                        </div>
                        <div class="job-text">
                            <p class="word-length-limit">{{x.compensation | compensation}}</p>
                            <p class="word-length-limit">{{x.name}}</p>
                            <p class="word-length-limit">{{x.companyName}}</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 job-content" ng-if="vm.selected">
                <div class="col-lg-3 col-md-6 padding-0" ng-repeat="x in vm.newestJob">
                    <div class="job-box">
                        <div class="job-logo">
                            <img ng-src="{{x.logo}}" alt="缩略图">
                        </div>
                        <div class="job-text">
                            <p class="word-length-limit">{{x.compensation | compensation}}</p>
                            <p class="word-length-limit">{{x.name}}</p>
                            <p class="word-length-limit">{{x.companyName}}</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 job-title">
                <a class="selected">推荐公司</a>
                <span class="pull-right">查看更多 ></span>
            </div>
            <div class="col-xs-12 company-content">
                <div class="col-lg-4 newest">
                    <img src="../img/zhiweihangye.png" alt="">
                    <div class="newest-text">
                        <p>{{vm.certificationCompany[0].name}}</p>
                        <p>{{vm.certificationCompany[0].slogan}}</p>
                        <div>
                            <span>
                                <img src="../img/zhiweishouji.png" alt="">&nbsp;
                                {{vm.certificationCompany[0].industryList[0] | industryList}}
                            </span>
                            <span>
                                 <img src="../img/zhiweirongzi.png" alt="">&nbsp;
                                {{vm.certificationCompany[0].financing | financing}}
                            </span>
                            <span>
                                <img src="../img/zhiweidizhi.png" alt="">&nbsp;
                                {{vm.certificationCompany[0].province | region}}
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-lg-8 col-md-12 padding-0 common-box">
                    <div class="col-sm-3 col-xs-6  padding-0" ng-repeat="x in vm.commonCompany">
                        <div class="common-item">
                            <img ng-src="{{x.logo}}" alt="">
                        </div>
                    </div>
                </div>
                <!--竖向轮播开始-->
                <div class="col-xs-12 vertical-swiper-box">
                    <div class="swiper-container vertical-swiper">
                        <div class="swiper-wrapper swiper-content">
                            <div class="swiper-slide" ng-repeat="x in vm.certificationCompany">
                                <div class="col-xs-6 company-box">
                                    <div class="company-logo">
                                        <img ng-src="{{x.logo}}" alt="">
                                    </div>
                                    <div class="company-text">
                                        <p class="word-length-limit">{{x.name}}</p>
                                        <p class="word-length-limit">{{x.slogan}}</p>
                                        <div class="word-length-limit">
                                            <span>{{x.industryList[0] | industryList}}</span>
                                            <span>{{x.financing | financing}}</span>
                                            <span>{{x.province | region}}</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-6 padding-0">
                                    <div class="company-jobs">
                                        <p class="word-length-limit">正在热招</p>
                                        <div class="word-length-limit">
                                            <span>产品经理</span>
                                            <span>产品经理</span>
                                        </div>
                                        <i></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="swiper-pagination"></div>
                    </div>
                </div>
                <!--竖向轮播结束-->
            </div>

        </div>
    </div>
</div>