<div class="container company-detail">
    <div class="row">
        <div class="company-detail-card col-xs-12">
            <img ng-src="{{vm.companyLogo}}" alt="" class="company-detail-logo">
            <div class="company-card-text">
                <div style="font-size: 20px; color: #2b3d51" class="company-name">
                    <strong>{{vm.companyName}}</strong>
                </div>
                <div style="font-size: 18px; color: #536a82" class="company-slogan">
                    <strong>{{vm.companySlogan}}</strong>
                </div>
            </div>
        </div>
        <!--公司详情-->
        <div class="company-detail-content">
            <div class="companyTab">
                <button class="company-detail-text">公司详情</button>
                <div class="company-production">
                    公司产品<img ng-src="{{vm.production}}" alt="">
                </div>
                <div class="company-introduction">
                    公司介绍
                    <div class="introduction-text">{{vm.companyIntroduction}}</div>
                </div>
            </div>
        </div>
        <!--在招职位-->
        <div class="company-detail-jobs">

        </div>
        <div class="company-message"></div>
        <div class="company-target"></div>
    </div>
</div>