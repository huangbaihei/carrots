@charset "UTF-8";
@import "../../../css/global.scss";
.separate-page {
  @include middle;
  justify-content: flex-end;
  flex-wrap: wrap;
  padding-bottom:60px;
  font-size:14px;
  & > li {
    height: 79px;
    @include middle;
    margin-left: 20px;
    &:nth-of-type(1),
    &:nth-of-type(3){
      input {
        height: 32px;
        width: 50px;
        text-align: center;
        margin: 0 2px;
      }
    }
    .pagination>li>a{
      margin:0 5px;
      background:#f5f5f7;
      color: #666666;
      border-radius:3px;
    }
    .pagination > .active > a{
      background-color: #fc2b47;
      border-color: #fc2b47;
      color:white;
    }
    .pagination .disabled .ng-binding {
      color: #cccccc !important;
    }
  }
}