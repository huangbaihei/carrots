angular.module('myApp').controller('findElitesCtrl', function (requestSvc) {
    var vm = this;
    //获取公司列表前8个
    requestSvc.getCompanyList({size: 8})
        .then(function (res) {
            console.log(res);
            if (res.data.code===0){
                vm.jobList = res.data.data;
            }
        })
});