* {
  box-sizing: border-box;
  font-weight: 400; }

html,
body {
  font-size: 62.5%; }

a,
a:hover,
a:visited,
a:focus,
a:active {
  text-decoration: none;
  color: #337ab7; }

button {
  cursor: pointer;
  border: 0;
  outline: 0 !important;
  background: #fff;
  -webkit-tap-highlight-color: transparent !important;
  -webkit-appearance: none !important; }

ul {
  list-style: none;
  margin: 0;
  padding: 0; }

.mg-bottom-15 {
  margin-bottom: 15px; }

.text-center {
  text-align: center !important; }

.text-right {
  text-align: right !important; }

.table-border {
  border: 1px solid #eee; }

th, td {
  text-align: center;
  vertical-align: middle !important; }

th {
  font-weight: 700; }

.padding-0 {
  padding: 0; }

.pd-left-50 {
  padding-left: 50px; }

